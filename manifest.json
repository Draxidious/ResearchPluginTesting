{
  "name": "GitHub Tool Tips",
  "version": "1",
  "description": "Helps newcomers navigate GitHub and successfully contribute to open source projects.",
  "browser_action": {
    "name": "GitHub Plugin Development",
    "icons": ["icon.png"],
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "content_scripts": [
    {
      "matches": [ "https://github.com/*"],
      "css": [ "styles.css"],
      "js": ["jquery.min.js", "Chart.bundle.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version" : 2,
  "content_security_policy": "script-src 'self' 'sha256-Cm8Z6dOt5LoFdNDqouQYEMWB0Z3D3bSMkhouBBPsByw='; object-src 'self'"
}
