{
  "name": "jQuery DOM",
  "version": "1",
  "description": "Manipulate the DOM when the page is done loading",
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "js": [ "jquery.min.js", "content.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
    "manifest_version" : 2
}
