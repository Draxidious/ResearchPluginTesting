{
    "name" : "GitHub ToolTips",
    "version" : "1",
    "permissions": ["storage","activeTab"],
    "description": "Build an Extension!",
    "background":
    {
        "scripts": ["background.js"]
    },
    "content_scripts": 
    [
        {
            "matches":["http://github*"],
            "js":["script.js"],
            "run_at": "document_end"
        }
    ],
    "manifest_version" : 2
}